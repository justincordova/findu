# Backend API Configuration
API_BASE_URL=https://api.finduapp.com

# Environment
ENV=development

# App Information
APP_NAME=FindU
APP_VERSION=1.0.0

# Firebase Configuration
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_APP_ID=1:123456789012:web:abcdef1234567890

# Stripe Configuration (Frontend - Publishable Key Only)
STRIPE_PUBLISHABLE_KEY=pk_test_XXXXXXXXXXXXXXXXXXXXXXXX

# Social Authentication (Client IDs only)
GOOGLE_OAUTH_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com
APPLE_CLIENT_ID=your.app.bundle.id
FACEBOOK_APP_ID=your-facebook-app-id

# Maps & Location
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Analytics (Frontend SDKs)
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Feature Flags (Frontend toggles)
FEATURE_SUPERLIKES_ENABLED=true
FEATURE_CAMPUS_BOOSTS_ENABLED=true
FEATURE_VIDEO_CALLS_ENABLED=false

# App Configuration (Frontend limits)
MAX_PHOTOS_PER_PROFILE=6
SUPER_LIKES_PER_DAY=5
MAX_MESSAGE_LENGTH=1000
DEFAULT_SEARCH_RADIUS=50
MIN_AGE=18
MAX_AGE=99

# Development
DEBUG_MODE=true